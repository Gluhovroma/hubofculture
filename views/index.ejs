              <div class="main-tags animated fadeIn" align="center" style="height: 45px;">
                  <div id="main-hook-menu" class="hook-item big-left" style="color:#292929;    float: left;       margin-top: 3px;" ng-click="showRadio();">           
                    <i class="ion-navicon-round" style="font-size: 35px;cursor: pointer;opacity: 0.8;padding: 5px;color:black; " ></i>    
                  </div>
                  <div id="main-hook-menu" class="hook-item small-left" style="    float: left;
    color: black;
    margin-top: 0px;
    width: 45px;
    height: 45px;
    padding: 0;
    margin: 0;
    padding-top: 3px;" ng-click="showRadio();">           
                    <i class="ion-headphone" style="font-size: 35px;cursor: pointer;opacity: 0.8;padding: 5px;color:black; " ></i>    
                  </div>  
                  <div class="main-tags-menu">
                    <a class="flow animated fadeIn" id="flow"  ui-sref="articles({'tag': 'all'})" ng-click="scrollTop();">поток</a>
                    <a class="people animated fadeIn" id="people" ui-sref="people" ng-click="scrollTop();">люди</a>
                    <a class="info animated fadeIn" id="info" ui-sref="info" ng-click="scrollTop();">о проекте</a>
                    <a class="tags-menu animated fadeIn" id="tags" href="" ng-click="showTags()" style="margin-right: 5px;">теги</a>                   
                  </div>
                  <div class="main-mobile-menu animated fadeIn" ng-bind="curState"></div>
                  <div id="enter" style="float:right;" ng-cloak>                    
                    <a  ng-click="login()" ng-show="user == null">                                          
                          <p class="vk-enter">Вход</p>                    
                    </a> 
                    <div class="user-info" ng-click="showUserMenu == true ? showUserMenu=false:showUserMenu=true" ng-cloak>                    
                        <img ng-src="{{user.photoUrl}}" ng-show="user != null" style="border-radius:50%;width: 40px;height: 40px;">                       
                          <span  id="username" ng-bind="user.username" style="color: #878685;text-transform: uppercase;letter-spacing: -1px;font-family: 'GothamProLight';font-weight: 400;
    font-size: 11px;color: #000000;line-height: 47px;">                         
                          </span> 
                          <i class="ion-ios-arrow-down" ng-show="user != null"></i>                                                            
                    </div>
                  </div>
                  <div id="right-hook" class="" ng-click="showSmallMenu()">            
                    <i class="ion-navicon-round"></i>     
                  </div>                   
              </div>                                  
              <div ng-style="{display:tagsDisplay}" style="display:none;padding-bottom: 5px;" align="center" class="main-menu-tags-container">
                <div class="main-menu-tag" ng-repeat="tag in tags" ng-cloak style="display:inline-block">               
                  <a ui-sref="articles({'tag': tag.urlname})" class="" ng-bind="::'#' + tag.name" ng-click="scrollTop();showTags()" style="color: #000000;"></a>
                </div>  
              </div>            
          </div>             
