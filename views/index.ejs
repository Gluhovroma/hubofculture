  <body  style="background: whitesmoke !important;width: 100%;" ng-controller="mainController">
    <main class="" id="main"  style="width:100%;">
      <div class="row"  style="margin:0">          
        <section id="radio" ng-swipe-left="closeRadio()" class="sidebar col-xs-alt-offset-12 col-xs-12 col-md-3 col-sm-5 animated fadeIn primary-info  slide-item left" ng-cloak>       
            <div class="col-md-12 site-info" style="position: relative;padding: 10px 0px 0px 0px;margin-left:0px;height: 100%; " ng-cloak> 
              <div id="main-hook-menu" class="hook-item radio-hook-item" style="position: absolute;
    font-size: 30px;
    z-index: 10;
    top: 3px;
    left: 4px;
    float: left;
    color: black;
    margin-top: 0px;
    width: 45px;
    height: 45px;
    padding: 0;
    margin: 0;
    padding-top: 3px;" ng-click="showRadio()">           
                    <i class="ion-close-round" style="font-size: 35px;
    cursor: pointer;
    
    padding: 5px;
    color: #f7f7f7;" ></i>     
                </div>     
      		    <div class="primary-info animated fadeIn "> 
                               
                <div id="main-player" class="player" align="center" ng-click="radioPlay()" style="cursor:pointer">					
							    <div id="player-wheels">
								    <div align="center" class="casette-wheel" ng-cloak>
								      <img src="/dk-fm/images/cs_wheel.png" class="" style="margin-left: 17%;" ng-class="{animation: animationCond}">
								    </div>                                    
								    <div align="center" class="casette-wheel" ng-cloak>
								      <img src="/dk-fm/images/cs_wheel.png" class="" style="margin-left: -14%; " ng-class="{animation: animationCond}">
								    </div>                                 
							    </div> 
							    <img id="casette-image" class="content-image-width" src="/dk-fm/images/cassette.png">							  
							    <div class="player-container" style="">
								    <div class="center-alingment col-xs-2 col-sm-2 col-md-2 col-lg-2">
									    <div class="table-wrap">
										    <div class="data-wrap">											    
										    </div>
									    </div>
								    </div>
								    <div class="center-alingment col-xs-8 col-sm-8 col-md-8 col-lg-8">
									    <div class="table-wrap">										    
									    </div>
								    </div>
  								 							
  							  </div>                            
						    </div>
            </div>
            <div class="player-controls" style="margin-top: 2px;">  
              <div id="audio-player" class="audio-player-wrapper" style="">
            <div class="audio-player-image" style="">
              <div class="song-name-container">
                <span class="audio-player-song-name" style="display:none"></span>
                 <span class="audio-player-song-name2" ng-bind="song"></span>                 
              </div>                           
            </div>
            <div class="audio-player-controls" style="text-align:center;margin-top: -5px;" ng-cloak>
              <div role="button" class="audio-player-button-waves " ng-class="{'ion-radio-waves-play':radioPlayCond}" ng-click="radioPlay()" style="margin-left: 4%;
    display: inline-block;
    float: left;
    font-size: 12px;
    text-align: center;margin-top: 7px;width: 0px;"><div style="display:inline-block;line-height: 15px;font-family: GothamProLight;" align="center">
    <div>
    	ON <br>AIR
    </div></div></div>
              <span class="audio-player-button-wrappers" style="display: inline-block;margin: 0px 0px 0px 0px;" ng-cloak> 

                <a role="button" class="audio-player-button  ion-ios-rewind" style="margin: 0;"></a>
                <!-- <i role="button" class="audio-player-button ion-ios-rewind" ng-click="playerPrev()" style="margin: 0;"></i> -->
                <i role="button" class="audio-player-button audio-player-place-pause-button ion-ios-play" ng-click="radioPlayPause()" style="margin: 0px 0px 0px 18px;" ng-cloak></i>
                <a role="button" class="audio-player-button ion-ios-fastforward" style="margin: 0;"></a>
